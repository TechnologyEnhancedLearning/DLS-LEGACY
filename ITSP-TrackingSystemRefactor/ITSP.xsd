                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CentreID, Active, RegionID, CentreName, ContactSurname + ', ' + ContactForename AS Contact, ContactEmail, ContactTelephone, PriorCandidates, AutoRegistered, BannerText, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts
                                 WHERE        (CustomPromptID = c.CustomField1PromptID)), '') AS F1Name, F1Mandatory, F1Options, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts AS CustomPrompts_2
                                 WHERE        (CustomPromptID = c.CustomField2PromptID)), '') AS F2Name, F2Mandatory, F2Options, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts AS CustomPrompts_1
                                 WHERE        (CustomPromptID = c.CustomField3PromptID)), '') AS F3Name, F3Mandatory, F3Options, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts AS CustomPrompts_5
                                 WHERE        (CustomPromptID = c.CustomField4PromptID)), '') AS F4Name, F4Mandatory, F4Options, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts AS CustomPrompts_4
                                 WHERE        (CustomPromptID = c.CustomField5PromptID)), '') AS F5Name, F5Mandatory, F5Options, COALESCE
                             ((SELECT        CustomPrompt
                                 FROM            CustomPrompts AS CustomPrompts_3
                                 WHERE        (CustomPromptID = c.CustomField6PromptID)), '') AS F6Name, F6Mandatory, F6Options, AutoRegisterManagerEmail,
                             (SELECT        RegionName
                               FROM            Regions
                               WHERE        (RegionID = c.RegionID)) AS RegionName, SignatureMimeType, SignatureImage, SignatureWidth, SignatureHeight, CentreLogo, LogoWidth, LogoHeight, LogoMimeType, BetaTesting, CentreTypeID,
                             (SELECT        CentreType
                               FROM            CentreTypes
                               WHERE        (CentreTypeID = c.CentreTypeID)) AS CentreType, IPPrefix, CentreCreated,
                             (SELECT        COUNT(*) AS Expr1
                               FROM            Candidates
                               WHERE        (CentreID = c.CentreID)) AS Delegates,
                             (SELECT        COUNT(Progress.ProgressID) AS Registrations
                               FROM            Progress INNER JOIN
                                                         Candidates AS Candidates_1 ON Progress.CandidateID = Candidates_1.CandidateID
                               WHERE        (Candidates_1.CentreID = c.CentreID)) AS CourseRegistrations,
                             (SELECT        COUNT(Progress_1.ProgressID) AS Completions
                               FROM            Progress AS Progress_1 INNER JOIN
                                                         Candidates AS Candidates_1 ON Progress_1.CandidateID = Candidates_1.CandidateID
                               WHERE        (Progress_1.Completed IS NOT NULL) AND (Candidates_1.CentreID = c.CentreID)) AS Completions,
                             (SELECT        COUNT(AssessAttempts.AssessAttemptID) AS Expr1
                               FROM            AssessAttempts INNER JOIN
                                                         Candidates AS Candidates_3 ON AssessAttempts.CandidateID = Candidates_3.CandidateID
                               WHERE        (Candidates_3.CentreID = c.CentreID)) AS AssessAttempts,
                             (SELECT        COUNT(AssessAttempts_1.AssessAttemptID) AS Expr1
                               FROM            AssessAttempts AS AssessAttempts_1 INNER JOIN
                                                         Candidates AS Candidates_4 ON AssessAttempts_1.CandidateID = Candidates_4.CandidateID
                               WHERE        (Candidates_4.CentreID = c.CentreID) AND (AssessAttempts_1.Status = 1)) AS AssessPassed,
                             (SELECT        SUM(e.Duration) AS Expr1
                               FROM            Sessions AS e INNER JOIN
                                                         Candidates AS Candidates_2 ON e.CandidateID = Candidates_2.CandidateID
                               WHERE        (Candidates_2.CentreID = c.CentreID)) / 60 AS LearningTimeHrs,
                             (SELECT        COUNT(*) AS Expr1
                               FROM            AdminUsers
                               WHERE        (CentreID = c.CentreID)) AS AdminUsers,
                             (SELECT        MAX(ADS.LoginTime) AS Expr1
                               FROM            AdminSessions AS ADS INNER JOIN
                                                         AdminUsers AS ADU ON ADS.AdminID = ADU.AdminID
                               WHERE        (ADU.CentreID = c.CentreID)) AS LastLogin,
                             (SELECT        COUNT(*) AS Expr1
                               FROM            AdminSessions AS ADS INNER JOIN
                                                         AdminUsers AS ADU ON ADS.AdminID = ADU.AdminID
                               WHERE        (ADU.CentreID = c.CentreID)) AS Logins,
                             (SELECT        MAX(Sessions.LoginTime) AS Expr1
                               FROM            Sessions INNER JOIN
                                                         Customisations ON Sessions.CustomisationID = Customisations.CustomisationID
                               WHERE        (Customisations.CentreID = c.CentreID)) AS LastLearnerLogin, ContractTypeID, CMSAdministrators, CMSManagers, CustomCourses, CCLicences, Trainers, ContractReviewDate, ServerSpaceBytes, 
                         ServerSpaceUsed
FROM            Centres AS c
ORDER BY CentreName</CommandText>
