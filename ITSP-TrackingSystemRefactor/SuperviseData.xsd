                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        S.SectionID, S.ApplicationID, S.SectionNumber, S.SectionName, SUM(CASE WHEN asp1.SupervisorVerificationRequested IS NULL THEN 0 ELSE 1 END) AS VerificationRequests, a.DisplayFormatID
FROM            aspProgress AS asp1 INNER JOIN
                         Progress AS p ON asp1.ProgressID = p.ProgressID INNER JOIN
                         Sections AS S INNER JOIN
                         Tutorials AS T ON S.SectionID = T.SectionID INNER JOIN
                         CustomisationTutorials AS CT ON T.TutorialID = CT.TutorialID ON asp1.TutorialID = T.TutorialID AND p.CustomisationID = CT.CustomisationID INNER JOIN
                         Customisations AS cu ON p.CustomisationID = cu.CustomisationID INNER JOIN
                         Applications AS a ON S.ApplicationID = a.ApplicationID LEFT OUTER JOIN
                         AssessAttempts AS aa ON p.ProgressID = aa.ProgressID AND S.SectionNumber = aa.SectionNumber
WHERE        (p.ProgressID = @ProgressID) AND (CT.Status = 1) AND (S.ArchivedDate IS NULL) OR
                         (p.ProgressID = @ProgressID) AND (S.ArchivedDate IS NULL) AND (CT.DiagStatus = 1) OR
                         (p.ProgressID = @ProgressID) AND (S.ArchivedDate IS NULL) AND (cu.IsAssessed = 1)
GROUP BY S.SectionID, S.ApplicationID, S.SectionNumber, S.SectionName, cu.IsAssessed, p.CandidateID, p.CustomisationID, a.DisplayFormatID
ORDER BY S.SectionNumber</CommandText>
                    <Parameters>
