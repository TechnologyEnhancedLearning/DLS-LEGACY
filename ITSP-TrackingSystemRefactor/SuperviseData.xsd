                    <CommandText>SELECT        p.ProgressID, ca.LastName + ', ' + ca.FirstName + ' (' + ca.CandidateNumber + ')' AS Name, a.ApplicationName + ' - ' + cu.CustomisationName AS Activity, p.FirstSubmittedTime AS Enrolled, 
