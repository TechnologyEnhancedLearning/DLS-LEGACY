                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        cu.CustomisationID, a.ApplicationName + ' - ' + cu.CustomisationName AS CourseName, cu.IsAssessed, dbo.CheckCustomisationSectionHasDiagnostic(cu.CustomisationID, 0) AS HasDiagnostic, 
                         dbo.CheckCustomisationSectionHasLearning(cu.CustomisationID, 0) AS HasLearning,
                             (SELECT        CategoryName
                               FROM            CourseCategories
                               WHERE        (CourseCategoryID = a.CourseCategoryID)) AS Category,
                             (SELECT        CourseTopic
                               FROM            CourseTopics
                               WHERE        (CourseTopicID = a.CourseTopicID)) AS Topic
FROM            Customisations AS cu INNER JOIN
                         Applications AS a ON cu.ApplicationID = a.ApplicationID
WHERE        (cu.CentreID = @CentreID) AND (cu.Active = 1) AND (a.ASPMenu = 1) AND (a.ArchivedDate IS NULL) AND (cu.CustomisationID NOT IN
                             (SELECT        CustomisationID
                               FROM            GroupCustomisations
                               WHERE        (GroupID = @GroupID))) AND (a.CourseCategoryID = @AdminCategoryID) OR
                         (cu.Active = 1) AND (a.ASPMenu = 1) AND (a.ArchivedDate IS NULL) AND (cu.CustomisationID NOT IN
                             (SELECT        CustomisationID
                               FROM            GroupCustomisations AS GroupCustomisations_2
                               WHERE        (GroupID = @GroupID))) AND (a.CourseCategoryID = @AdminCategoryID) AND (cu.AllCentres = 1) AND EXISTS
                             (SELECT        CentreApplicationID
                               FROM            CentreApplications
                               WHERE        (ApplicationID = cu.ApplicationID) AND (CentreID = @CentreID) AND (Active = 1)) OR
                         (cu.CentreID = @CentreID) AND (cu.Active = 1) AND (a.ASPMenu = 1) AND (a.ArchivedDate IS NULL) AND (cu.CustomisationID NOT IN
                             (SELECT        CustomisationID
                               FROM            GroupCustomisations AS GroupCustomisations_1
                               WHERE        (GroupID = @GroupID))) AND (CAST(@AdminCategoryID AS Int) = 0) OR
                         (cu.Active = 1) AND (a.ASPMenu = 1) AND (a.ArchivedDate IS NULL) AND (cu.CustomisationID NOT IN
                             (SELECT        CustomisationID
                               FROM            GroupCustomisations AS GroupCustomisations_1
                               WHERE        (GroupID = @GroupID))) AND (CAST(@AdminCategoryID AS Int) = 0) AND (cu.AllCentres = 1) AND EXISTS
                             (SELECT        CentreApplicationID
                               FROM            CentreApplications AS CentreApplications_1
                               WHERE        (ApplicationID = cu.ApplicationID) AND (CentreID = @CentreID) AND (Active = 1))
ORDER BY Category, Topic, cu.CustomisationID</CommandText>
                    <Parameters>
