                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT (SELECT Applications.ApplicationName
              FROM    Applications INNER JOIN
                            Customisations AS CS2 ON Applications.ApplicationID = CS2.ApplicationID
              WHERE  (CS2.CustomisationID = CS.CustomisationID)) AS AppName,
                 (SELECT Applications_1.PLAssess
                 FROM    Applications AS Applications_1 INNER JOIN
                              Customisations AS CS3 ON Applications_1.ApplicationID = CS3.ApplicationID
                 WHERE (CS3.CustomisationID = CS.CustomisationID)) AS HasAssess,
                 (SELECT App.DiagAssess
                 FROM    Applications AS App INNER JOIN
                              Customisations AS CS3 ON App.ApplicationID = CS3.ApplicationID
                 WHERE (CS3.CustomisationID = CS.CustomisationID)) AS HasDiag, CustomisationID, Active, CurrentVersion, CentreID, ApplicationID, CustomisationName, CustomisationText, IsAssessed, IsStandard, Password, SelfRegister,
                 (SELECT COUNT(CustomisationID) AS Expr1
                 FROM    Progress AS p1
                 WHERE (CustomisationID = CS.CustomisationID) AND (Completed IS NULL)) AS CandidateCountInProgress, TutCompletionThreshold, DiagCompletionThreshold, DiagObjSelect, CASE WHEN
                 (SELECT App1.LaunchedAssess
                 FROM    Applications AS App1 INNER JOIN
                              Customisations AS CS3 ON App1.ApplicationID = CS3.ApplicationID
                 WHERE (CS3.CustomisationID = CS.CustomisationID)) = 1 THEN 1 WHEN
                 (SELECT ct.BetaTesting
                 FROM    Centres AS ct INNER JOIN
                              Customisations AS cu ON ct.CentreID = cu.CentreID
                 WHERE (cu.CustomisationID = @CustomisationID)) = 1 THEN 1 ELSE 0 END AS AssessAvailable,
                 (SELECT COUNT(ProgressID) AS cs
                 FROM    Progress AS P2
                 WHERE (CustomisationID = CS.CustomisationID) AND (NOT (Completed IS NULL))) AS Completions, HideInLearnerPortal, CompleteWithinMonths, Mandatory, ValidityMonths, AutoRefresh, RefreshToCustomisationID, AutoRefreshMonths, Q1Options, Q2Options, Q3Options, 
             CourseField1PromptID, CourseField2PromptID, CourseField3PromptID, COALESCE
                 ((SELECT CoursePrompt
                  FROM    CoursePrompts
                  WHERE (CoursePromptID = CS.CourseField1PromptID)), '') AS CField1, COALESCE
                 ((SELECT CoursePrompt
                  FROM    CoursePrompts AS CoursePrompts_2
                  WHERE (CoursePromptID = CS.CourseField2PromptID)), '') AS CField2, COALESCE
                 ((SELECT CoursePrompt
                  FROM    CoursePrompts AS CoursePrompts_1
                  WHERE (CoursePromptID = CS.CourseField3PromptID)), '') AS CField3, DisableCompletion, InviteContributors, CreatedDate,
                 (SELECT MAX(SubmittedTime) AS LastAccess
                 FROM    Progress
                 WHERE (CustomisationID = CS.CustomisationID)) AS LastAccess, NotificationEmails, ApplyLPDefaultsToSelfEnrol
FROM   Customisations AS CS
WHERE (CustomisationID = @CustomisationID)</CommandText>
